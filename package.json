{
  "name": "karakul",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/byteark-project/karakul.git",
  "author": "Eva <690090823@qq.com>",
  "license": "MIT",
  "scripts": {
    "start": "next",
    "prebuild": "node scripts/version.js",
    "build": "next build",
    "prod": "next start",
    "icon:merge": "svg-sprite-generate -d src/images/ -o public/images/sprite.svg",
    "lint:tsx": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|json|yml|yaml|scss|less|css|md)\"",
    "schema:update": "env-cmd -f .env.development graphql-codegen --config src/services/codegen.dev.yml",
    "schema": "graphql-codegen --config src/services/codegen.local.yml"
  },
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@apollo/client": "^3.1.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.5.4",
    "antd-dayjs-webpack-plugin": "^1.0.1",
    "babel-plugin-import": "^1.13.0",
    "cookie": "^0.4.1",
    "graphql": "^15.3.0",
    "js-cookie": "^2.2.1",
    "less": "^3.12.2",
    "less-vars-to-js": "^1.3.0",
    "moment": "^2.27.0",
    "next": "^9.5.2",
    "node-sass": "^4.14.1",
    "nprogress": "^0.2.0",
    "null-loader": "^4.0.0",
    "object-hash": "^2.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "use-onclickoutside": "^0.3.1",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/import-types-preset": "^1.17.9",
    "@graphql-codegen/schema-ast": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@types/cookie": "^0.4.0",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^14.6.0",
    "@types/nprogress": "^0.2.0",
    "@types/object-hash": "^1.3.3",
    "@types/react": "^16.9.46",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "@zeit/next-typescript": "^1.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.7.0",
    "eslint-config-like": "^1.3.0",
    "prettier": "^2.0.5",
    "svg-sprite-generator": "^0.0.7",
    "typescript": "^4.0.2"
  },
  "lint-staged": {
    "src/**/*.+(js|jsx|ts|tsx|json|yml|yaml|scss|less|css|md)": [
      "npm run format"
    ],
    "src/**/*.{ts,tsx}": [
      "npm run lint:tsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
